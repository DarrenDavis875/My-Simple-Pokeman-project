body{background-color:rgb(170,170,238);font-family:'Franklin Gothic Medium','Arial Narrow',Arial,sans-serif;font-weight:700}.btn{background:blue;color:#ccc;margin-bottom:5px;width:200px;height:50px;border:2px solid;font-size:24px;border-radius:10px;text-align:center}.page-header__logo{display:block;margin-left:auto;margin-right:auto;padding:50px 25px 25px}.pokemon_list{display:grid;grid-template-columns:repeat(auto-fit,minmax(200px,1fr));grid-gap:10px;padding:20px;list-style-type:none}